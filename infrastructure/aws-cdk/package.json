{
  "name": "live-commerce-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK Infrastructure for Live Commerce Streaming",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:dev": "cdk deploy LiveCommerceStreamingStack-dev",
    "deploy:prod": "cdk deploy LiveCommerceStreamingStack-prod",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "destroy": "cdk destroy --all"
  },
  "keywords": ["aws-cdk", "live-streaming", "rtmp", "hls"],
  "author": "Live Commerce Team",
  "license": "MIT",
  "dependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "aws-cdk": "^2.120.0"
  }
}
